{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "types": [
      "jasmine",
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@app/*": ["app/*"],
      "@styles/*": ["styles/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "../**/*.unit.ts",
    "../**/*.e2e.ts",
    "../**/*.pageObject.ts"
  ]
}
